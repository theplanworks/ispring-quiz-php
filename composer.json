{
    "name": "theplan/ispring-quiz-php",
    "description": "A PHP package to read quiz results from iSpring QuizMaker",
    "keywords": [
        "thePLAN",
        "ispring-quiz-php"
    ],
    "homepage": "https://github.com/theplan/ispring-quiz-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Fox",
            "email": "matt.fox@theplanworks.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^1.20",
        "laravel/pint": "^1.2",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "ThePLAN\\IspringQuizPhp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ThePLAN\\IspringQuizPhp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
